{
  "name": "project",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "admin",
    "server",
    "storefront"
  ],
  "scripts": {
    "dev": "concurrently \"cd admin && yarn start\" \"cd server && yarn start:dev\" \"cd storefront && yarn start\"",
    "build:admin": "cd admin && yarn build",
    "build:server": "cd server && yarn build",
    "build:storefront": "cd storefront && yarn build",
    "p:studio": "cd server && prisma studio",
    "p:format": "cd server && prisma format",
    "db:push": "cd server && prisma db push"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "@types/lodash": "4.14.194"
  },
  "devDependencies": {
    "concurrently": "8.0.1"
  }
}